<div class="container">
    <form action="results" method="GET" id="newsletter">
        <div class="columns is-centered">
            <div class="column is-one-third">
                <input class="input" type="text" placeholder="Name" name="name" />
            </div>

            <div class="column is-one-third">
                <button class="button is-link is-fullwidth">Search</button>
            </div>
        </div>
    </form>

    <div>
    {{#each giphyHttml}}
        <figure class="image is-128x128">
            <img src="{{this}}">
        </figure>
    {{/each}}
    </div>

    <br><br>
</div>